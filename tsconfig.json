{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "moduleResolution": "node",
    "allowJs": true,
    "noEmit": true,
    "strict": true,
    "esModuleInterop": true,
    "typeRoots": [
      "./custom_typings",
      "./node_modules/@types"
    ],
  },
  "include": [
    "**/*"
  ]
}
